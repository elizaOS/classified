{
  "name": "@elizaos/platform",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev:cloud": "npx next dev -p 3333",
    "build": "NEXT_PHASE=phase-production-build npx next build",
    "build:prod": "bun run lint && bun run build",
    "start": "npx next start -p 3333",
    "start:standalone": "node scripts/standalone-server.js",
    "dev:standalone": "node scripts/build-client.js && node scripts/standalone-server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "eslint . --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "test": "bun test",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:isomorphic": "NODE_ENV=test jest tests/isomorphic-auth.test.ts",
    "test:build": "NODE_ENV=test jest tests/build-integration.test.ts",
    "test:pwa": "NODE_ENV=test jest tests/pwa-integration.test.ts",
    "test:platform": "NODE_ENV=test jest tests/platform-integration.test.ts",
    "test:all": "NODE_ENV=test jest tests/isomorphic-auth.test.ts tests/build-integration.test.ts tests/pwa-integration.test.ts tests/platform-integration.test.ts",
    "test:integration-runtime": "NODE_ENV=test jest tests/integration/real-runtime.test.ts",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3333 cypress",
    "test:e2e:headless": "start-server-and-test dev http://localhost:3333 cypress:headless",
    "test:e2e:real": "start-server-and-test dev http://localhost:3333 \"cypress run --spec 'cypress/e2e/REAL-*.cy.ts'\"",
    "test:e2e:real-gui": "start-server-and-test dev http://localhost:3333 \"cypress open --spec 'cypress/e2e/REAL-*.cy.ts'\"",
    "test:integration": "cypress run --spec 'cypress/e2e/client-integration.cy.ts'",
    "test:standalone": "cypress run --spec 'cypress/e2e/standalone-server.cy.ts'",
    "test:standalone-e2e": "start-server-and-test start:standalone http://localhost:3000 test:standalone",
    "test:production": "node scripts/run-production-tests.js",
    "test:production:quick": "cypress run --spec 'cypress/e2e/00-FINAL-PRODUCTION-VALIDATION.cy.ts'",
    "test:api-keys": "cypress run --spec 'cypress/e2e/03-api-keys-complete.cy.ts,cypress/e2e/03-api-keys-edge-cases.cy.ts'",
    "test:coverage-report": "cypress run --spec 'cypress/e2e/00-COMPLETE-TEST-COVERAGE-REPORT.cy.ts'",
    "deploy": "node scripts/deploy-web.js",
    "deploy:vercel": "DEPLOY_TARGET=vercel node scripts/deploy-web.js",
    "deploy:netlify": "DEPLOY_TARGET=netlify node scripts/deploy-web.js",
    "deploy:static": "DEPLOY_TARGET=static node scripts/deploy-web.js",
    "docker:build": "docker build -t elizaos-platform .",
    "docker:run": "docker run -p 3000:3000 elizaos-platform",
    "docker:compose": "docker-compose up -d",
    "docker:compose:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.855.0",
    "@aws-sdk/s3-request-presigner": "^3.855.0",
    "@headlessui/react": "^2.1.10",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.1.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@stripe/react-stripe-js": "^2.9.0",
    "@stripe/stripe-js": "^4.10.0",
    "@tauri-apps/api": "^2.3.0",
    "@types/js-yaml": "^4.0.9",
    "@types/mdx": "^2.0.13",
    "@types/mime-types": "^3.0.1",
    "@workos-inc/node": "^7.0.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.2.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "ethers": "^6.15.0",
    "ioredis": "^5.4.1",
    "isomorphic-dompurify": "^2.26.0",
    "jose": "^5.9.6",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.453.0",
    "mime-types": "^3.0.1",
    "next": "15.3.4",
    "next-auth": "^4.24.11",
    "next-plausible": "^3.12.4",
    "next-pwa": "^5.6.0",
    "posthog-js": "^1.195.0",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.3.5",
    "react-hot-toast": "^2.5.2",
    "sonner": "^1.5.0",
    "stripe": "^17.3.1",
    "swagger-ui-react": "^5.11.0",
    "tailwind-merge": "^2.5.4",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "ws": "^8.18.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.13.0",
    "@next/eslint-plugin-next": "^15.3.4",
    "@testing-library/cypress": "^10.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/crypto-js": "^4.2.2",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/swagger-ui-react": "^4.18.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "cypress": "^13.15.0",
    "eslint": "^9.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^15.3.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "glob": "^11.0.3",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "start-server-and-test": "^2.0.8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5",
    "typescript-eslint": "^8.12.2"
  }
}
