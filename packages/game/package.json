{
  "name": "@elizaos/game",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "description": "Autonomous coding game where AI agents self-improve through code generation",
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "bun build src-backend/server.ts --outdir=dist-backend --target=node",
    "build:frontend": "vite build",
    "preview": "vite preview",
    "test": "node scripts/run-comprehensive-tests.js",
    "dev": "bun run build && concurrently \"bun run dev:backend\" \"bun run dev:frontend\"",
    "dev:backend": "bun run src-backend/server.ts",
    "dev:backend:node": "node --loader tsx dist-backend/server.js",
    "dev:frontend": "vite dev",
    "dev:force-restart": "npm run kill-processes && npm run dev",
    "kill-processes": "node scripts/kill-processes.js",
    "launch": "bun run src-backend/launch-game.ts",
    "launch:container": "bun run src-backend/launch-game.ts --container",
    "launch:direct": "bun run src-backend/launch-game.ts"
  },
  "dependencies": {
    "@elizaos/api-client": "workspace:*",
    "@elizaos/core": "1.2.12",
    "@elizaos/plugin-autocoder": "workspace:*",
    "@elizaos/plugin-autonomy": "workspace:*",
    "@elizaos/plugin-bootstrap": "workspace:*",
    "@elizaos/plugin-e2b": "workspace:*",
    "@elizaos/plugin-experience": "workspace:*",
    "@elizaos/plugin-goals": "workspace:*",
    "@elizaos/plugin-knowledge": "workspace:*",
    "@elizaos/plugin-openai": "^1.0.0",
    "@elizaos/plugin-personality": "workspace:*",
    "@elizaos/plugin-shell": "workspace:*",
    "@elizaos/plugin-sql": "workspace:*",
    "@elizaos/plugin-stagehand": "workspace:*",
    "@elizaos/plugin-todo": "workspace:*",
    "@elizaos/plugin-vision": "workspace:*",
    "@elizaos/server": "workspace:*",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@types/express-fileupload": "^1.5.1",
    "aws-sdk": "^2.1692.0",
    "commander": "^14.0.0",
    "concurrently": "^8.2.2",
    "express-fileupload": "^1.5.2",
    "mock-aws-s3": "^4.0.2",
    "nock": "^14.0.6",
    "react": "^19.1.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.47.0",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "^1.2.17",
    "@types/node": "^24.0.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.3.4",
    "cypress": "^13.6.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "happy-dom": "^18.0.1",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "vite": "^6.0.3",
    "vitest": "^2.0.0"
  },
  "gitHead": "b165ad83e5f7a21bc1edbd83374ca087e3cd6b33"
}
