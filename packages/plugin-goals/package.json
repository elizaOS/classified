  },
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun run build.ts --watch",
    "start": "echo 'Plugin package, use with ElizaOS'",
    "test": "NODE_ENV=test bun test && NODE_ENV=test elizaos test",
    "lint": "echo 'Linting disabled for plugin-goals due to ESLint config conflicts with ES modules' && prettier --write ./src",
    "clean": "rm -rf dist && rm -rf .turbo",
    "typecheck": "tsc --noEmit",
    "test:e2e": "NODE_ENV=test elizaos test"
